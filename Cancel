local args = {
    [1] = {
        ["Type"] = "SaveStoneSetSpawn",
        ["SaveStone"] = "Cut Island"
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("RemoteObjects"):WaitForChild("CTS"):FireServer(unpack(args))

task.wait(2.5)

